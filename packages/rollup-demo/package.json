{
  "name": "rollup-demo",
  "packageManager": "yarn@3.6.0",
  "scripts": {
    "build-js": "rollup -c",
    "build-css": "postcss client/styles --dir build/styles",
    "watch-js": "rollup -c -w",
    "watch-css": "postcss client/styles --dir build/styles --watch",
    "dev": "npm-run-all --parallel watch-js watch-css"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@rollup/plugin-babel": "^6.0.3",
    "core-js": "^3.32.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "regenerator-runtime": "^0.13.11",
    "rollup": "^3.28.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2"
  }
}
