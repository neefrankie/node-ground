{
  "name": "rollup-demo",
  "packageManager": "yarn@3.6.0",
  "scripts": {
    "watch-js": "rollup -c -w",
    "watch-css": "postcss client/main.css --output build/main.css --watch",
    "dev": "npm-run-all --parallel watch-js watch-css"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "rollup": "^3.25.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2"
  }
}
